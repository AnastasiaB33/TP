@model CadastrProject.Models.Removal_Request
@{
    ViewBag.Title = "Details";
}

<h2>Детальный просмотр</h2>

<div>
    <h4>Запись</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            № заявки
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt>
            Дата заявки на снятие
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Date_request)
        </dd>
        <dt>
            № кадастра
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IDCadastr)
        </dd>
        <dt>
            Тип объекта
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Group.Type)
        </dd>
        <dt>
            Адрес
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Address)
        </dd>
        <dt>
            Кадастровая стоимость
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Value)
        </dd>
        <dt>
            Площадь
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Square)
        </dd>
        <dt>
            Дата подачи заявки на регистрацию объекта 
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Date_application)
        </dd>
        <dt>
           Дата регистрации
       </dt>
        <dd>
           @Html.DisplayFor(model => model.Date_registration)
        </dd>
        <dt>
            Владелец
        </dt>
        <dd>
            @Html.DisplayFor(model => model.AspNetUsers.Surname)
            @Html.DisplayFor(model => model.AspNetUsers.Name)
            @Html.DisplayFor(model => model.IDUser)
        </dd>
        <dt>
            Дата снятия с учета
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DateDelete)
        </dd>
        <dt>
            Статус
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Status.Name)
        </dd>
        <dt>
            Причина
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Cause)
        </dd>

    </dl>

</div>
<p>
    @Html.ActionLink("Вернуться на главную", "../Home/Index")
</p>